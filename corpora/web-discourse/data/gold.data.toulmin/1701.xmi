. "/><type2:DocumentMetaData xmi:id="13" sofa="6" begin="0" end="694" language="en" documentId="1701" documentUri="1701" isLastSegment="false"/><types:WebArgumentMetadata xmi:id="24" sofa="6" begin="0" end="0" author="Oh LORD" date="03/29/2009 01:46 PM" docType="artcomment" origUrl="http://www.forerunner.com/forerunner/X0098_Ban_on_school_prayer.html#c000530" topic="prayer-in-schools" thumbsUp="0" thumbsDown="0" origId="1701" notes="" title="Banning Prayer in Public Schools Has Led to America's Demise"/><type4:Token xmi:id="39" sofa="6" begin="0" end="3"/><type4:Token xmi:id="47" sofa="6" begin="3" end="4"/><type4:Token xmi:id="55" sofa="6" begin="5" end="9"/><type4:Token xmi:id="63" sofa="6" begin="10" end="12"/><type4:Token xmi:id="71" sofa="6" begin="13" end="15"/><type4:Token xmi:id="79" sofa="6" begin="16" end="21"/><type4:Token xmi:id="87" sofa="6" begin="21" end="22"/><type4:Token xmi:id="95" sofa="6" begin="23" end="33"/><type4:Token xmi:id="103" sofa="6" begin="34" end="41"/><type4:Token xmi:id="111" sofa="6" begin="42" end="47"/><type4:Token xmi:id="119" sofa="6" begin="48" end="55"/><type4:Token xmi:id="127" sofa="6" begin="56" end="58"/><type4:Token xmi:id="135" sofa="6" begin="59" end="65"/><type4:Token xmi:id="143" sofa="6" begin="66" end="71"/><type4:Token xmi:id="151" sofa="6" begin="72" end="77"/><type4:Token xmi:id="159" sofa="6" begin="78" end="81"/><type4:Token xmi:id="167" sofa="6" begin="82" end="84"/><type4:Token xmi:id="175" sofa="6" begin="85" end="91"/><type4:Token xmi:id="183" sofa="6" begin="91" end="92"/><type4:Token xmi:id="191" sofa="6" begin="93" end="96"/><type4:Token xmi:id="199" sofa="6" begin="97" end="105"/><type4:Token xmi:id="207" sofa="6" begin="105" end="106"/><type4:Token xmi:id="215" sofa="6" begin="107" end="110"/><type4:Token xmi:id="223" sofa="6" begin="111" end="113"/><type4:Token xmi:id="231" sofa="6" begin="114" end="116"/><type4:Token xmi:id="239" sofa="6" begin="117" end="121"/><type4:Token xmi:id="247" sofa="6" beg