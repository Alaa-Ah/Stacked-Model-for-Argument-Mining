13" sofa="6" begin="0" end="657" language="en" documentId="1621" documentUri="1621" isLastSegment="false"/><types:WebArgumentMetadata xmi:id="24" sofa="6" begin="0" end="0" author="riley" docType="forumpost" origUrl="http://www.opposingviews.com/questions/should-prayer-be-allowed-in-public-schools#comment-8083" topic="prayer-in-schools" thumbsUp="0" thumbsDown="0" origId="1621" notes="" title="Should Prayer Be Allowed in Public Schools?"/><type4:Token xmi:id="39" sofa="6" begin="0" end="2"/><type4:Token xmi:id="47" sofa="6" begin="3" end="4"/><type4:Token xmi:id="55" sofa="6" begin="5" end="12"/><type4:Token xmi:id="63" sofa="6" begin="12" end="13"/><type4:Token xmi:id="71" sofa="6" begin="14" end="15"/><type4:Token xmi:id="79" sofa="6" begin="16" end="21"/><type4:Token xmi:id="87" sofa="6" begin="22" end="29"/><type4:Token xmi:id="95" sofa="6" begin="30" end="36"/><type4:Token xmi:id="103" sofa="6" begin="37" end="47"/><type4:Token xmi:id="111" sofa="6" begin="48" end="55"/><type4:Token xmi:id="119" sofa="6" begin="56" end="62"/><type4:Token xmi:id="127" sofa="6" begin="63" end="65"/><type4:Token xmi:id="135" sofa="6" begin="66" end="73"/><type4:Token xmi:id="143" sofa="6" begin="74" end="75"/><type4:Token xmi:id="151" sofa="6" begin="76" end="78"/><type4:Token xmi:id="159" sofa="6" begin="79" end="85"/><type4:Token xmi:id="167" sofa="6" begin="86" end="88"/><type4:Token xmi:id="175" sofa="6" begin="89" end="97"/><type4:Token xmi:id="183" sofa="6" begin="98" end="101"/><type4:Token xmi:id="191" sofa="6" begin="102" end="107"/><type4:Token xmi:id="199" sofa="6" begin="108" end="111"/><type4:Token xmi:id="207" sofa="6" begin="112" end="116"/><type4:Token xmi:id="215" sofa="6" begin="117" end="125"/><type4:Token xmi:id="223" sofa="6" begin="126" end="130"/><type4:Token xmi:id="231" sofa="6" begin="131" end="139"/><type4:Token xmi:id="239" sofa="6" begin="140" end="142"/><type4:Token xmi:id="247" sofa="6" begin="143" end="144"/><type4:Token xmi:id="255" sofa="6" begin=