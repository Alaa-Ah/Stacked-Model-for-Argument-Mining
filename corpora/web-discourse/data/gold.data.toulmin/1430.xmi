Segment="false"/><types:WebArgumentMetadata xmi:id="24" sofa="6" begin="0" end="0" author="WaterBuilder" docType="artcomment" origUrl="http://www.cbc.ca/news/canada/toronto/tempers-flare-over-prayer-in-schools-1.1104775" topic="prayer-in-schools" thumbsUp="0" thumbsDown="0" origId="1430" notes="" title="Tempers flare over prayer in schools"/><type4:Token xmi:id="39" sofa="6" begin="0" end="4"/><type4:Token xmi:id="47" sofa="6" begin="5" end="7"/><type4:Token xmi:id="55" sofa="6" begin="8" end="12"/><type4:Token xmi:id="63" sofa="6" begin="13" end="22"/><type4:Token xmi:id="71" sofa="6" begin="22" end="23"/><type4:Token xmi:id="79" sofa="6" begin="24" end="28"/><type4:Token xmi:id="87" sofa="6" begin="28" end="29"/><type4:Token xmi:id="95" sofa="6" begin="30" end="37"/><type4:Token xmi:id="103" sofa="6" begin="38" end="42"/><type4:Token xmi:id="111" sofa="6" begin="43" end="47"/><type4:Token xmi:id="119" sofa="6" begin="48" end="53"/><type4:Token xmi:id="127" sofa="6" begin="54" end="59"/><type4:Token xmi:id="135" sofa="6" begin="59" end="60"/><type4:Token xmi:id="143" sofa="6" begin="61" end="63"/><type4:Token xmi:id="151" sofa="6" begin="64" end="65"/><type4:Token xmi:id="159" sofa="6" begin="66" end="69"/><type4:Token xmi:id="167" sofa="6" begin="70" end="75"/><type4:Token xmi:id="175" sofa="6" begin="76" end="78"/><type4:Token xmi:id="183" sofa="6" begin="79" end="82"/><type4:Token xmi:id="191" sofa="6" begin="83" end="92"/><type4:Token xmi:id="199" sofa="6" begin="93" end="95"/><type4:Token xmi:id="207" sofa="6" begin="96" end="97"/><type4:Token xmi:id="215" sofa="6" begin="98" end="104"/><type4:Token xmi:id="223" sofa="6" begin="105" end="114"/><type4:Token xmi:id="231" sofa="6" begin="115" end="122"/><type4:Token xmi:id="239" sofa="6" begin="122" end="123"/><type4:Token xmi:id="247" sofa="6" begin="125" end="129"/><type4:Token xmi:id="255" sofa="6" begin="129" end="130"/><type4:Token xmi:id="263" sofa="6" begin="131" end="133"/><type4:Token xmi:id="271" sofa="