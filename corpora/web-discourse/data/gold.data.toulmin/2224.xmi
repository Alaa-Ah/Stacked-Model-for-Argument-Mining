ould prefer... &#10;Btw, I work at a co-ed school at the moment and the kids there get on just fine."/><type2:DocumentMetaData xmi:id="13" sofa="6" begin="0" end="780" language="en" documentId="2224" documentUri="2224" isLastSegment="false"/><types:WebArgumentMetadata xmi:id="24" sofa="6" begin="0" end="0" author="JimmyRay" date="2012-May-16, 10:26 pm" docType="forumpost" origUrl="http://whrl.pl/RdbG8O" topic="single-sex-education" thumbsUp="0" thumbsDown="0" origId="2224" notes="" title="Girls to Co-Ed or all girls high school"/><type4:Token xmi:id="39" sofa="6" begin="0" end="1"/><type4:Token xmi:id="47" sofa="6" begin="2" end="6"/><type4:Token xmi:id="55" sofa="6" begin="7" end="9"/><type4:Token xmi:id="63" sofa="6" begin="10" end="12"/><type4:Token xmi:id="71" sofa="6" begin="13" end="16"/><type4:Token xmi:id="79" sofa="6" begin="17" end="21"/><type4:Token xmi:id="87" sofa="6" begin="22" end="28"/><type4:Token xmi:id="95" sofa="6" begin="29" end="30"/><type4:Token xmi:id="103" sofa="6" begin="31" end="34"/><type4:Token xmi:id="111" sofa="6" begin="34" end="35"/><type4:Token xmi:id="119" sofa="6" begin="35" end="36"/><type4:Token xmi:id="127" sofa="6" begin="37" end="40"/><type4:Token xmi:id="135" sofa="6" begin="41" end="42"/><type4:Token xmi:id="143" sofa="6" begin="43" end="55"/><type4:Token xmi:id="151" sofa="6" begin="56" end="61"/><type4:Token xmi:id="159" sofa="6" begin="62" end="64"/><type4:Token xmi:id="167" sofa="6" begin="64" end="65"/><type4:Token xmi:id="175" sofa="6" begin="66" end="67"/><type4:Token xmi:id="183" sofa="6" begin="68" end="73"/><type4:Token xmi:id="191" sofa="6" begin="74" end="76"/><type4:Token xmi:id="199" sofa="6" begin="77" end="81"/><type4:Token xmi:id="207" sofa="6" begin="82" end="91"/><type4:Token xmi:id="215" sofa="6" begin="92" end="96"/><type4:Token xmi:id="223" sofa="6" begin="97" end="99"/><type4:Token xmi:id="231" sofa="6" begin="100" end="101"/><type4:Token xmi:id="239" sofa="6" begin="102" end="107"/><type4:Token xmi:i