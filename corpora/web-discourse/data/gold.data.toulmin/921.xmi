en. "/><type2:DocumentMetaData xmi:id="13" sofa="6" begin="0" end="682" language="en" documentId="921" documentUri="921" isLastSegment="false"/><types:WebArgumentMetadata xmi:id="24" sofa="6" begin="0" end="0" author="AlexJones" date="06 November 2009 4:04pm" docType="artcomment" origUrl="http://discussion.theguardian.com/comment-permalink/6745316" topic="homeschooling" thumbsUp="1" thumbsDown="0" origId="921" notes="" title="Ridiculous rules for home schools"/><type4:Token xmi:id="39" sofa="6" begin="0" end="1"/><type4:Token xmi:id="47" sofa="6" begin="1" end="2"/><type4:Token xmi:id="55" sofa="6" begin="2" end="3"/><type4:Token xmi:id="63" sofa="6" begin="4" end="7"/><type4:Token xmi:id="71" sofa="6" begin="8" end="9"/><type4:Token xmi:id="79" sofa="6" begin="10" end="13"/><type4:Token xmi:id="87" sofa="6" begin="14" end="16"/><type4:Token xmi:id="95" sofa="6" begin="17" end="20"/><type4:Token xmi:id="103" sofa="6" begin="21" end="26"/><type4:Token xmi:id="111" sofa="6" begin="27" end="32"/><type4:Token xmi:id="119" sofa="6" begin="32" end="33"/><type4:Token xmi:id="127" sofa="6" begin="34" end="37"/><type4:Token xmi:id="135" sofa="6" begin="38" end="39"/><type4:Token xmi:id="143" sofa="6" begin="40" end="43"/><type4:Token xmi:id="151" sofa="6" begin="44" end="47"/><type4:Token xmi:id="159" sofa="6" begin="48" end="51"/><type4:Token xmi:id="167" sofa="6" begin="52" end="55"/><type4:Token xmi:id="175" sofa="6" begin="56" end="66"/><type4:Token xmi:id="183" sofa="6" begin="67" end="69"/><type4:Token xmi:id="191" sofa="6" begin="70" end="75"/><type4:Token xmi:id="199" sofa="6" begin="76" end="80"/><type4:Token xmi:id="207" sofa="6" begin="80" end="81"/><type4:Token xmi:id="215" sofa="6" begin="82" end="85"/><type4:Token xmi:id="223" sofa="6" begin="86" end="93"/><type4:Token xmi:id="231" sofa="6" begin="94" end="96"/><type4:Token xmi:id="239" sofa="6" begin="96" end="97"/><type4:Token xmi:id="247" sofa="6" begin="98" end="102"/><type4:Token xmi:id="255" sofa="6" begi